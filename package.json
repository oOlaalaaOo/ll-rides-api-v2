{
  "name": "ll-rides-api",
  "version": "1.0.0",
  "description": "An expressjs server for restful api of ll-rides app",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "build": "rm -rf ./dist && tsc",
    "serve": "node dist/server.js",
    "tsc": "tsc -- --init"
  },
  "keywords": [
    "express",
    "typescript",
    "restful-api",
    "node",
    "server"
  ],
  "author": "espanola.benjamin@gmail.com",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.7.8",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.7.1",
    "@types/socket.io": "^2.1.4",
    "@types/winston": "^2.4.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.1",
    "typescript": "^3.7.5"
